figure, figcaption, footer, header, hgroup, 
menu, nav, output, ruby, section, summary,
time, mark, audio, video {
	margin: 0;
	padding: 0;
	border: 0;
	font-size: 100%;
	font: inherit;
	vertical-align: baseline;
}
/* HTML5 display-role reset for older browsers */
article, aside, details, figcaption, figure, 
footer, header, hgroup, menu, nav, section {
	display: block;
}
body {
	line-height: 1;
}
ol, ul {
	list-style: none;
}
blockquote, q {
	quotes: none;
}
blockquote:before, blockquote:after,
q:before, q:after {
	content: '';
	content: none;
}
table {
	border-collapse: collapse;
	border-spacing: 0;
}

/*-------------------------------------------------
  0 - Variables
---------------------------------------------------*/

$gh-grey: #b1b6b9;
$heading: "adelle";
$body: "rooney-web";
$cyan: #00aeef;

// Button animation
@mixin transition($value){
	-webkit-transition: all $value linear;
		-moz-transition: all $value linear;
			-o-transition: all $value linear;
				transition: all $value linear;
}

/*-------------------------------------------------
  1 - Structure 
 -------------------------------------------------*/

body {
	background: #fff;
	font-size: 62.5%;
}

#logo a img {
	width: 120px;
}

header {
	float: left;
	margin: 30px;
	position: fixed;
	width: 120px;
}

ul#menu-nav {
	font-size: 1.6em;
	margin-top: 20px;
	text-align: center;
	li a {
		@include transition(0.05s);
		color: #000;
		display: inline-block;
		font-family: $body;
		padding: 10px;
		text-decoration: none;

		&:hover {
			@include transition(0.1s);
			background: $gh-grey;
			color: #fff;
		}
	}
}

#content {
	float: left;
	margin: 20px 0 30px 160px;
}

footer {
	font-family: $body;
	color: $gh-grey;
	font-size: 14px;
	position: fixed;
	bottom: 0;
	left: 0;
	text-align: left;
	width: 180px;
}

/*-------------------------------------------------
  2 - Portfolio 
 -------------------------------------------------*/

.entry {
	.portfolio_short_content {
		margin-left: -10px;
		color: $gh-grey;

		h1 {
			border-bottom: 2px dotted black;
			padding: 0.6em 0;
			margin-bottom: 0.8em;
			a {
				@include transition(0.15s);
				font-family: $heading;
				color: #000;
				font-size: 2.35em;
				text-decoration: none;
				&:hover {
					@include transition(0.15s);
					color: $cyan;
				}
			}
		}

		.portfolio_terms {
			text-transform: capitalize;
			color: $gh-grey;
			a {	
				font-size: 0.8em;
				color: $gh-grey;
				font-family: $heading;
				&:hover {
					color: black;
					text-decoration: underline;
				}
			}
			margin-bottom: 20px;
		}

	}
	.portfolio_thumb {
		margin-left: -10px;
		padding: 15px 0px 35px 0px;
		cursor: pointer;;
	}
	a#main_img{
		opacity: 1;
		&:hover{
			opacity: 0.85;
		}
	}
}

/*-------------------------------------------------
  3 - Portfolio (Description) 
 -------------------------------------------------*/

#content{
	.breadcrumbs{
		margin: 18px 87px 0 30px;
		padding: 0.04em 0 0.23em;
		border-bottom: 2px dotted black;
		font-family: $heading;
		color: #000;
		font-size: 3.53em;
		text-decoration: none;
	}
	.technologies {
		font-size: 13px;
		margin: 2px 87px 0 30px;
		padding-top: 1em;
		text-transform: capitalize;
		color: $gh-grey;
		a {	
			font-size: 12px;
			color: $gh-grey;
			font-family: $heading;
			&:hover {
				color: black;
				font-family: $heading;
				text-decoration: underline;
			}
		}
		margin-bottom: 17px;
	}

	.entry {
		padding: 0 20px;
		margin: 0 0 0 10px;
		.portfolio_thumb{
			img {
				width: 100%;
			}

			.lightbox{
				display: inline-block;
				a{
					float: left;
					opacity: 1;
				}
				&:hover{
					opacity: 0.85;
				}
			}		 
			.portfolio_images_block{
				display: inline-block;
				ul.portfolio_images_rows{
					width: 330px;
					list-style: none;
					margin-left: 1px;
					a{
						border:none;
					}
					li{
						padding: 1px 2px 0px 2px;
						float: left;
						opacity: 1;
						&:hover{
							opacity: 0.85;
						}
					}
				}
			}
		}
	}

	.portfolio_short_content{
		font-family: $body;
		font-size: 15px;
		h4{
			line-height: 1.5em;
			margin: 0 430px 20px 30px;
		}
		p{
			color: $gh-grey;
			margin: 0 430px 5px 30px;
		}
		.lable{
			color: $gh-grey;
			font-weight: bold;
			font-style: italic;
		}
	}
}
/*-------------------------------------------------
  4 - About
 -------------------------------------------------*/

.entry-content{
	font-size: 2em;
	padding: 10px 10px;
	h2{
		margin: 0 1.2em 0 0.4em;			 
		border-top: 2px dotted black;
		border-bottom: 2px dotted black;
		color: $gh-grey;
		padding: 0.5em 3em 0.5em 0;
		font-family: $heading;
		font-size: 2.5em;
		font-weight: 300;
		line-height: 1em;
	}
	h3{	
		font-family: $body;
		color: $gh-grey;
		font-weight: 300;
		font-size: 1.5em;
		line-height: 1.15em;
		margin: 1em 7em 1em 0.75em;
	}
	h4{
		font-family: $body;
		color: black;
		font-size: 0.75em;
		line-height: 1.5em;
		margin: 1em 16em 0em 1.5em;
	}
	.downloadbtn{
		background-image: url('../images/pdf_icon.png');
		background-repeat: no-repeat;
		background-position: 10px 10px;
		background-size: 20px 26px;
		color: white;
		background-color: rgba(0, 174, 239, 1);
		font-family: $body;
		font-size: 15px;
		text-indent: 24px;
		padding: 1em 1em;
		text-transform: uppercase;
		float: right;
		text-decoration: none;
		margin: 0em 4em; 
		&:hover {
			cursor: pointer;
			background-color: rgba(0, 174, 239, 0.5);
		}
	}
}

/*-------------------------------------------------
  5 - Contact
 -------------------------------------------------*/
.entry-content{
	.contact{
		width: 32.5%;
		line-height: 1.5em;
		font-family: $body;
		font-size: 0.75em;
		margin: 25px 0 0 400px;
		a {
			color: #000;
			text-decoration: none;
			border-bottom: 1px solid $cyan;
			&:hover {
				color: $cyan;
			}
		}
		h3{
			font-family: $heading;
			margin: 1em 0 0.3em 0;
			font-size: 1.65em;
		}
	}
	.wpcf7{
		margin: 30px 0 0 22px;
		width: 320px;
		background-color: $gh-grey;
		padding: 0.8em;
		color: white;
		font-family: $body;
		float: left;
		display: inline-block;
		font-size: 0.8em;
	}
	input[type="text"], input[type="email"]{
		margin: 10px 0;
		border: 2px solid white;
		height: 20px;
		width: 310px;
		font-family: $body;
		font-size: 0.8em;
		padding: 0.2em 0.2em;
	}
	textarea{
		margin: 10px 0;
		border: 2px solid white;
		width: 310px;
		height: 120px;
		padding: 0.2em 0.2em;
		resize: none;
		font-family: $body;
		font-size: 13px;
	}
	input[type="submit"]{
		border: none;
		color: white;
		background: rgba(0, 174, 239, 1);
		font-family: $body;
		font-size: 15px;
		padding: 0.3em 1em;
		text-transform: uppercase;
		float: right;
		&:hover {
			cursor: pointer;
			background: rgba(0, 174, 239, 0.5);
		}
	}
	ul#social_media{
		margin-top: 0.3em;
		list-style: none;
		a{
			border: none;
		}
		li{
			float: left;
			margin-right: 12px;
			opacity: 0.5;
			
			&:hover {
				cursor: pointer;
				opacity: 1;
			}
		}
	}
}

/*-------------------------------------------------
  6 - Technology
 -------------------------------------------------*/
.entry {
	.portfolio_short_content_technology{
		padding: 2em 0 0.4em; 
		font-size: 15px;
		color: $gh-grey;
		font-family: $body;
		.lable{
			color: $gh-grey;
			font-weight: bold;
			font-style: italic;
		}
		.item_title{
			font-family: $heading;
			font-size: 20px;
			padding-bottom: 0.6em;
			a {
				color: black;
				text-decoration: none;
				border-bottom: 1px solid $cyan;
				&:hover {
					color: $cyan;
				}
			}
		}
	}
}

 /*-------------------------------------------------
  9 - Miscellaneous 
 -------------------------------------------------*/

.clearfix {
	clear: both;
}
/*-------------------------------------------------
  7 - Media Queries 
 -------------------------------------------------*/

/* Tablet Devices, Desktop and Laptop Screens */
@media only screen and (max-width: 768px) 	{
	header {
		float: none;
		margin: 20px 35px;
		position: static;
		width: 100%;

		#logo {
			float: left;
		}
	}

	ul#menu-nav {
		margin: 50px 20px;
		float: left;
		text-align: left;
		li {
			float: left;
			margin: 0 1em;
			a {
				display: inline-block;
				font-family: $body;
				padding: 10px;
			}
		}
	}

	#content {
		float: none;
		margin: 30px 0;
		.breadcrumbs{
			margin: 0 30px;
			padding: 5.13em 0 0.24em;
			font-size: 3em;
		}
		.technologies {
			padding-top: 0.8em;
		}
		.portfolio_short_content{
			h4{
				line-height: 1.5em;
				margin: 0 30px 70px 30px;
			}
			p{
				margin: 0 0px 5px 30px;
			}
		}
	}

	.entry-content {
		h1{
			font-size: 0.85em;
		}
		h2{
			font-size: 2em;
			margin: 10px 20px;
			padding: 30px 45px 30px 0px; 
		}
		h3{
			font-size: 1.2em;
			margin: 30px 20px;
			padding-right: 50px; 
		}
		h4{
			margin: 20px 20px 10px 20px;
			padding-right: 60px; 
		}
		.downloadbtn{
			margin: 15px 20px 30px 0;
			padding-right: 18px; 
		}
		.wpcf7{
			margin: 20px 0 20px 22px;
			font-size: 0.75em;
		}
		.contact{
		width: 26%;
		line-height: 1.5em;
		font-family: $body;
		font-size: 0.7em;
		margin: 25px 0 0 380px;
			a {
				color: #000;
				text-decoration: none;
				border-bottom: 1px solid $cyan;
				&:hover {
					color: $cyan;
				}
			}
			h3{
				font-family: $heading;
				margin: 1em 0 0.3em 0;
				font-size: 1.6em;
			}
		}
	}
	footer{
		position: static;
		margin: -50px auto;
	}
}

/* Smartphones (portrait and landscape) ----------- */
@media only screen and (min-width : 320px) and (max-width : 480px) {
	header {
		#logo {
			float: none;
			width: 200px;
			margin: 0px auto;
		}
	}

	ul#menu-nav {
		margin: 20px 10px;
		float: none;
		text-align: left;
		li {
			float: left;
			margin: 0 0.8em;
			a {
				display: inline-block;
				font-family: $body;
				padding: 10px;
			}
		}
	}

	#content {
		float: none;
		margin: 70px 0 30px 0;
		.breadcrumbs{
			margin: 0 30px;
			padding: 0.7em 0 0.22em;
			font-size: 2.1em;
		}
		.technologies {
			padding-top: 0.7em;
		}		
	}

	.entry-content { 
		h1{
			font-size: 0.6em;
		}
		h2{
			font-size: 1.3em;
			margin: 5px 20px;
			padding: 20px 0; 
		}
		h3{
			font-size: 1em;
			margin: 10px 20px;
			padding: 0; 
		}
		h4{
			font-size: 0.7em;
			margin: 20px 20px 10px 20px;
			padding: 0; 
		}
		.downloadbtn{
			float: none;
			display: block;
			width: 175px;
			margin: 30px auto;
		}
		.wpcf7{
			margin: 20px 0 0 22px;
			font-size: 0.75em;
			width: 290px;
			padding: 0.5em;
		}
		input[type="text"], input[type="email"]{
			margin: 7px 0;
			border: 1px solid white;
			width: 284px;
		}
		textarea{
			margin: 7px 0;
			border: 1px solid white;
			width: 284px;
		}
		input[type="submit"]{
			font-size: 14px;
		}
		.contact{
		float: left;
		width: 78%;
		line-height: 1.5em;
		font-family: $body;
		font-size: 0.7em;
		margin: 20px 0 20px 22px;
			a {
				color: #000;
				text-decoration: none;
				border-bottom: 1px solid $cyan;
				&:hover {
					color: $cyan;
				}
			}
			h3{
				font-family: $heading;
				margin: 1em 0 0.3em 0;
				font-size: 1.5em;
			}
		}
	}

}
