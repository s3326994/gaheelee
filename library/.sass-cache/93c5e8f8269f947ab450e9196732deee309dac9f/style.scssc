article, aside, canvas, details, embed, 
figure, figcaption, footer, header, hgroup, 
menu, nav, output, ruby, section, summary,
time, mark, audio, video {
	margin: 0;
	padding: 0;
	border: 0;
	font-size: 100%;
	font: inherit;
	vertical-align: baseline;
}
/* HTML5 display-role reset for older browsers */
article, aside, details, figcaption, figure, 
footer, header, hgroup, menu, nav, section {
	display: block;
}
body {
	line-height: 1;
}
ol, ul {
	list-style: none;
}
blockquote, q {
	quotes: none;
}
blockquote:before, blockquote:after,
q:before, q:after {
	content: '';
	content: none;
}
table {
	border-collapse: collapse;
	border-spacing: 0;
}

/*-------------------------------------------------
  0 - Variables
---------------------------------------------------*/

$gh-grey: #b1b6b9;
$heading: "adelle";
$body: "rooney-web";
$cyan: #00aeef;

// Button animation
@mixin transition($value){
	-webkit-transition: all $value linear;
		-moz-transition: all $value linear;
			-o-transition: all $value linear;
				transition: all $value linear;
}

/*-------------------------------------------------
  1 - Structure 
 -------------------------------------------------*/

body {
	background: #fff;
}

#logo a img {
	width: 120px;
}

header {
	float: left;
	margin: 30px 20px;
	position: fixed;
	width: 120px;
}
ul#menu-nav {
	margin-top: 20px;
	text-align: center;
	li a {
		@include transition(0.05s);
		color: #000;
		display: inline-block;
		font-family: $body;
		padding: 10px;
		text-decoration: none;

		&:hover {
			@include transition(0.1s);
			background: $gh-grey;
			color: #fff;
		}
	}
}

#content {
	float: left;
	margin: 30px 0 30px 195px;
	width: 80%;
}

footer {
	font-family: $body;
	color: $gh-grey;
	font-size: 14px;
	position: fixed;
	bottom: 0;
	left: 0;
	text-align: left;
	width: 180px;
}

/*-------------------------------------------------
  2 - Portfolio 
 -------------------------------------------------*/

.entry {
	.portfolio_short_content {
		margin-top: -7px;
		color: $gh-grey;
		h1 {
			width: 1022px;
			border-bottom: 2px dotted black;
			padding: 0.6em 0;
			margin-bottom: 0.8em;
			a {
				@include transition(0.15s);
				font-family: $heading;
				color: #000;
				font-size: 35px;
				text-decoration: none;
				&:hover {
					@include transition(0.15s);
					color: $cyan;
				}
			}
		}
		.portfolio_terms {
			font-size: 13px;
			text-transform: capitalize;
			color: $gh-grey;
			a {	
				font-size: 12px;
				color: $gh-grey;
				font-family: $heading;
				&:hover {
					color: black;
					font-family: $heading;
					text-decoration: underline;
				}
			}
			margin-bottom: 20px;
		}
	}
	.portfolio_thumb {
		padding: 15px 0px 35px 0px;
		cursor: pointer;;
	}
	a#main_img{
		opacity: 1;
		&:hover{
			opacity: 0.85;
		}
	}
}

/*-------------------------------------------------
  3 - Portfolio (Description) 
 -------------------------------------------------*/

#content{
	.breadcrumbs{
		padding: 0.04em 0 0.25em;
		border-bottom: 2px dotted black;
		width: 1022px;
		font-family: $heading;
		color: #000;
		font-size: 35px;
		text-decoration: none;
	}
	.technologies {
		font-size: 13px;
		padding-top: 0.95em;
		text-transform: capitalize;
		color: $gh-grey;
		a {	
			font-size: 12px;
			color: $gh-grey;
			font-family: $heading;
			&:hover {
				color: black;
				font-family: $heading;
				text-decoration: underline;
			}
		}
		margin-bottom: 21px;
	}

	.entry{
		.portfolio_thumb{
			width: 1000px;
			.lightbox{
				display: inline-block;
				a{
				float: left;
				opacity: 1;
				}
				&:hover{
					opacity: 0.85;
				}
			}		 
			.portfolio_images_block{
				display: inline-block;
				ul.portfolio_images_rows{
					width: 330px;
					list-style: none;
					margin-left: 1px;
					a{
						border:none;
					}
					li{
						padding: 1px 2px 0px 2px;
						float: left;
						opacity: 1;
						&:hover{
							opacity: 0.85;
						}
					}
				}
			}
		}
	}

	.portfolio_short_content{
		color: black;
		font-family: $body;
		font-size: 15px;
		width: 650px;
		h4{
			line-height: 1.5em;
		}
		p{
			color: $gh-grey;
			margin-bottom: 0.3em;
		}
		.lable{
			color: $gh-grey;
			font-weight: bold;
			font-style: italic;
		}
	}
}
/*-------------------------------------------------
  4 - About
 -------------------------------------------------*/

.entry-content{
	width: 1000px;
	h2{
		width: 850px;
		border-top: 2px dotted black;
		border-bottom: 2px dotted black;
		color: $gh-grey;
		padding: 0.5em 3em 0.5em 0;
		font-family: $heading;
		font-size: 50px;
		font-weight: 300;
	}
	h3{
		width: 850px;
		padding: 1em 6em 1em 0;
		font-family: $body;
		font-size: 30px;
		color: $gh-grey;
		font-weight: 300;
		line-height: 1.15em;
	}
	h4{
		width: 830px;
		padding: 0.2em 12em 0.5em 0;
		font-family: $body;
		font-size: 15px;
		color: black;
		line-height: 1.5em;
	}
	.downloadbtn{
		background-image: url('../images/pdf_icon.png');
		background-repeat: no-repeat;
		background-position: 10px 10px;
		background-size: 20px 26px;
		color: white;
		background-color: rgba(0, 174, 239, 1);
		font-family: $body;
		font-size: 15px;
		text-indent: 24px;
		padding: 1em 1em;
		text-transform: uppercase;
		float: right;
		text-decoration: none;
		&:hover {
			cursor: pointer;
			background-color: rgba(0, 174, 239, 0.5);
		}
	}
}

/*-------------------------------------------------
  5 - Contact
 -------------------------------------------------*/
.entry-content{
	.contact{
		width: 360px;
		float: left;
		display: inline;
		line-height: 1.5em;
		margin: 18px 30px 0;
		font-family: $body;
		font-size: 15px;
		a {
			color: #000;
			text-decoration: none;
			border-bottom: 1px solid $cyan;
			&:hover {
				color: $cyan;
			}
		}
		h3{
			width: 150px;
			color: $gh-grey;
			font-family: $heading;
			font-size: 25px;
			padding: 0.2em 0;
			margin-top: 0.5em;
		}
	}
	.wpcf7{
		margin: 25px 0 0 2px;
		width: 320px;
		background-color: $gh-grey;
		padding: 0.8em;
		color: white;
		font-family: $body;
		float: left;
	}
	input[type="text"], input[type="email"]{
		margin: 10px 0;
		border: 2px solid white;
		height: 20px;
		width: 310px;
		font-family: $body;
		font-size: 13px;
		padding: 0.2em 0.2em;
	}
	textarea{
		margin: 10px 0;
		border: 2px solid white;
		width: 310px;
		height: 120px;
		padding: 0.2em 0.2em;
		resize: none;
		font-family: $body;
		font-size: 13px;
	}
	input[type="submit"]{
		border: none;
		color: white;
		background: rgba(0, 174, 239, 1);
		font-family: $body;
		font-size: 15px;
		padding: 0.3em 1em;
		text-transform: uppercase;
		float: right;
		&:hover {
			cursor: pointer;
			background: rgba(0, 174, 239, 0.5);
		}
	}
	ul#social_media{
		margin-top: 0.3em;
		list-style: none;
		a{
			border: none;
		}
		li{
			float: left;
			margin-right: 12px;
			opacity: 0.5;
			
			&:hover {
				cursor: pointer;
				opacity: 1;
			}
		}
	}
}

/*-------------------------------------------------
  6 - Technology
 -------------------------------------------------*/
.entry{
	.portfolio_short_content_technology{
		padding: 2em 0 0.4em; 
		font-size: 15px;
		color: $gh-grey;
		font-family: $body;
		.lable{
			color: $gh-grey;
			font-weight: bold;
			font-style: italic;
		}
		.item_title{
			font-family: $heading;
			font-size: 20px;
			padding-bottom: 0.6em;
			a {
				color: black;
				text-decoration: none;
				border-bottom: 1px solid $cyan;
				&:hover {
					color: $cyan;
				}
			}
		}
	}
}

 /*-------------------------------------------------
  9 - Miscellaneous 
 -------------------------------------------------*/

.clearfix {
	clear: both;
}
